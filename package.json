    "babel-runtime": "^6.26.0",
    "base-64": "^0.1.0",
    "base32": "0.0.6",
    "bn.js": "^4.11.8",
    "create-hmac": "^1.1.7",
    "diffie-hellman": "^5.0.3",
    "elliptic": "^6.4.0",
